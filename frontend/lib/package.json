{
    "name": "@streamlit/lib",
    "version": "1.24.1",
    "private": true,
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "sideEffects": false,
    "scripts": {
        "build": "yarn build:src && yarn build:vendor && yarn build:assets && yarn build:proto && yarn build:lib-type",
        "build:lib-type": "tsc -P tsconfig.json && tsc-alias -p tsconfig.json",
        "build:tsc": "tsc -P tsconfig.json",
        "build:tsc-alias": "tsc-alias -p tsconfig.json",
        "build:src": "env NODE_ENV=\"development\" ./node_modules/.bin/babel --root-mode=upward --out-dir dist --source-maps --extensions \".js,.mjs,.jsx,.ts,.tsx\" src",
        "build:vendor": "cp -R ./src/vendor ./dist",
        "build:assets": "cp -R ./src/assets ./dist",
        "build:proto": "cp ./src/proto* ./dist",
        "typecheck": "tsc",
        "test": "env NODE_OPTIONS=--max_old_space_size=8192 jest --env=../jest/jsdom-polyfill-env.js --runTestsByPath ${TESTPATH}",
        "testWatch": "env NODE_OPTIONS=--max_old_space_size=8192 jest --watchAll --env=../jest/jsdom-polyfill-env.js --runTestsByPath ${TESTPATH}",
        "test:coverage": "env NODE_OPTIONS=--max_old_space_size=8192 jest --env=../jest/jsdom-polyfill-env.js --coverage --watchAll false --watch false ./",
        "lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx --max-warnings 0 src"
    },
